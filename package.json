{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "cross-env VITE_APP_ENV=prod npm run build",
    "build:dev": "cross-env VITE_APP_ENV=dev npm run build",
    "preview": "vite preview",
    "preview:dev": "vite preview --base=/dev/",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fontsource-variable/roboto-flex": "^5.2.6",
    "@mdi/font": "^7.4.47",
    "@toast-ui/calendar": "^2.1.3",
    "axios": "^1.10.0",
    "core-js": "^3.43.0",
    "pinia": "^3.0.3",
    "toast-ui-calendar-vue3": "^0.3.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-toastification": "2.0.0-rc.5",
    "vuetify": "^3.8.12"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.1",
    "eslint": "^9.30.1",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.7.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "cypress",
      "esbuild",
      "vue-demi"
    ]
  }
}
